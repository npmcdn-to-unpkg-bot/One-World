  <div class="available-housing test">
  <h2>Available Housing in Toronto</h2>

    <div class="row">
      <% @listings.each_with_index do |listing, index| %>
        <div class="col s12 m7 l4">
          <div class="card large">
            <div class="card-image" id="listings-card">
              <%= image_tag listing.listing_images.first.picture %>
            </div>
            <span class="card-title">
              <%= listing.headline %>
            </span>
            <div class="card-content">
              <%= listing.description.truncate(80) %>
            <br>
              <p> Accommodates: <%= listing.accommodates %> </p>
            </div>
            <div class="card-action">
              <%= link_to "#{@letters[index]}: View Home ", listing %>
            </div>
          </div>
        </div>
      <% end %>
    </div>

</div>
